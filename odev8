3-UPDATE employee
SET name = 'ASD fG',
  birthday = '1999-05-01',
  email = 'asd@fd.com'
WHERE id = 21;

UPDATE employee
SET name = 'Mann Ous',  
  birthday = '1988-06-06',
  email = 'mas@os.com'
WHERE id = 32;

