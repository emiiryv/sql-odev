4-
DELETE FROM employee
WHERE name ILIKE '%a%a%'	
RETURNING *;

DELETE FROM employee
WHERE id>48;

DELETE FROM employee
WHERE 
	(id>18 AND id<28) OR		
	(email LIKE '%.cz')
RETURNING *;

DELETE FROM employee
WHERE name LIKE 'Ro%'
RETURNING *;

DELETE FROM employee
WHERE id =2;	
